<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!-- Izinkan semua koneksi HTTP (cleartext) untuk semua domain dan IP -->
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates src="system" />
        </trust-anchors>
    </base-config>
</network-security-config>
